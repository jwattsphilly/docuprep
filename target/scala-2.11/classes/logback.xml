<configuration>
	<appender name="FILE" class="ch.qos.logback.core.FileAppender">
		<file>AddPDF_logFile.log</file>
		<append>true</append>
		<encoder>
			<pattern>%d{MMM dd yyyy HH:mm:ss} %-5level %logger{35} - %msg%n</pattern>
		</encoder>
	</appender>
	<root level="INFO">
		<appender-ref ref="FILE" />
	</root>
</configuration>